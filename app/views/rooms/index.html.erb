<h1 class="text-5xl font-extrabold">Hi, <%= current_user.username %><h1>

<h2 class="text-4xl font-extrabold">Users<h2>
<ul>
  <% @users.each do |user| %>
    <li><%= user.username %></li>
  <% end %>
</ul>

<div data-controller="room-list">
  <h2 class="text-4xl font-extrabold">Rooms<h2>

  <%= render 'form' %>

  <ul data-room-list-target="list">
    <%= render @rooms %>
  </ul>
</div>
